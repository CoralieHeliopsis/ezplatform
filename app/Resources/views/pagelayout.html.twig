<!doctype html>

<html lang="fr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>Liste des dépôts GitHub d'Heliopsis</title>

    <!-- style list -->

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/repolist/css/bootstrap.min.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/repolist/css/bootstrap-theme.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/repolist/css/style.css') }}" rel="stylesheet">
    {% endblock %}
</head>

<body>

<!-- navigation -->

<nav class="navbar navbar-default navbar-fixed-top" id="total-navbar">
    <div class="container">
        <!-- Brand and toggle -->
        <div class="navbar-header page-scroll">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#depot-navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">
                <img alt="Brand" src="{{ asset('bundles/repolist/images/heliopsis.png') }}">
            </a>
        </div>

        <!-- nav links -->
        <div id="depot-navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li><a href="#">ttttt</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div><!-- /.container -->
</nav>

<!-- header -->

<header class="below-navbar">
    <div class="container">
        <div class="row" id="banner">
            <div class="col-xs-12">
            </div>
        </div>
    </div>
</header>

<!-- content -->

<section class="buttons">
    <div class="container">
        <div class="row regular-content-size">
            <div class="col-xs-6 col-xs-offset-3">
                <h4 class="center bottom">Recherchez un dépôt</h4>
                <div class="input-group custom-search-form">
                    <input type="text" class="form-control">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="button">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>
                </div>
            </div>
        </div>
    </div>
</section>

{% block content %}{% endblock %}


<!-- footer -->

<footer class="text-center">
    <div class="footer-color-one">
        <div class="container">
            <div class="col-xs-10 col-xs-offset-1 text-center" id="intouch"></div>
        </div>
    </div>
    <div class="footer-color-two">
        <div class="container">
            <div class="row regular-content-size">
                <div class="col-xs-8 col-xs-offset-2 footer-links-block">
                    <ul class="list-inline footer-links">
                        <li><a href="#">Heliopis</a></li>
                        <li><a href="#">Nous contacter</a></li>
                        <li><a href="#">FAQ</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-color-three">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <p class="small">Website développé avec eZ Platform</p>
                </div>
            </div>
        </div>
    </div>
</footer>

<!-- script JS -->

{#{% block javascript %}#}
{#<script src="{{ asset('bundles/repolist/js/bootstrap.min.js') }}"></script>#}
{#{%  endblock %}#}

</body>

</html>